{
  "name": "biosky-shared",
  "version": "0.1.0",
  "description": "Shared types, auth, and database utilities for BioSky",
  "license": "AGPL-3.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./database": {
      "types": "./dist/database/index.d.ts",
      "import": "./dist/database/index.js"
    },
    "./generated": {
      "types": "./dist/generated/index.d.ts",
      "import": "./dist/generated/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@atproto/api": "^0.18.15",
    "@atproto/jwk-jose": "^0.1.0",
    "@atproto/lexicon": "^0.6.0",
    "@atproto/oauth-client-node": "^0.3.15",
    "@atproto/xrpc": "^0.7.0",
    "express": "^4.18.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "typescript": "^5.3.0"
  }
}
